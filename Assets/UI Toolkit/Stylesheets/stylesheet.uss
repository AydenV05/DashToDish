.Button {
    position: relative;
    left: auto;
    right: auto;
    top: auto;
    bottom: auto;
    visibility: visible;
    overflow: visible;
    align-items: flex-end;
    justify-content: flex-start;
    margin-left: auto;
    margin-right: auto;
    margin-top: auto;
    margin-bottom: auto;
    padding-left: 10px;
    padding-right: 10px;
    padding-top: 10px;
    padding-bottom: 10px;
    color: rgba(255, 255, 255, 0);
    background-color: rgba(115, 106, 106, 255);
}
